[application]

config/name="The bug adventure show"
config/description="A graphic adventure for the blockchain. A multi-game-jam-entry for the Game Off, AdvXJam 2021, Theta Hackathon Q3 2021 and GoGodotJam 2021."
run/main_scene="res://popochiu/Rooms/Lobby/RoomLobby.tscn"
config/icon="res://icon.png"

[autoload]

NetworkManager="*res://networking/NetworkManager.gd"
WebsocketManager="*res://networking/WebsocketManager.gd"
Utils="*res://addons/Popochiu/Engine/Others/Utils.gd"
Cursor="*res://addons/Popochiu/Engine/Cursor/Cursor.tscn"
E="*res://addons/Popochiu/Engine/Popochiu.tscn"
C="*res://addons/Popochiu/Engine/Interfaces/ICharacter.gd"
I="*res://addons/Popochiu/Engine/Interfaces/IInventory.gd"
D="*res://addons/Popochiu/Engine/Interfaces/IDialog.gd"
G="*res://addons/Popochiu/Engine/Interfaces/IGraphicInterface.gd"
A="*res://addons/Popochiu/Engine/AudioManager/AudioManager.tscn"
Globals="*res://popochiu/Globals.gd"
Console="*res://addons/quentincaffeino/console/src/Console.tscn"

[debug]

gdscript/warnings/enable=false

[display]

window/size/width=320
window/size/height=180
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor]

script_templates_search_path="res://addons/Popochiu/Engine/Templates"

[editor_plugins]

enabled=PoolStringArray( "res://addons/Popochiu/plugin.cfg", "res://addons/lospec_palette_list/plugin.cfg", "res://addons/quentincaffeino/console/plugin.cfg" )

[gui]

theme/custom_font="res://popochiu/GraphicInterface/Resources/DynamicFonts/MonkeyIsland-16.tres"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
