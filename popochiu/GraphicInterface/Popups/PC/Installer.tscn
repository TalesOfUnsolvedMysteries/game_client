code = "/*
Squish Sprite Shader by Garmelon (https://github.com/Garmelon)
Documented by Mateus-Carmo31
*/

shader_type canvas_item;

uniform bool uv_mode = false;


void fragment() {
	vec2 uv = UV;
	float value = mod(TIME*10.0, 22.0);
	float alpha = 1.0;
	if (value < 1.0) {
		alpha = 0.0;
	} else if (value<5.0) {
		uv.x -= 0.005;
	} else if (value<6.0) {
		alpha = 0.0;
	} else if (value < 10.0) {
		// nothing
	} else if(value < 11.0) {
		alpha = 0.0;
	} else if (value < 15.0) {
		uv.x += 0.005;
	} else if (value < 16.0) {
		alpha = 0.0;
	} else if (value < 21.0) {
		// nothing
	}
	COLOR = texture(TEXTURE, uv);
	COLOR.a = alpha;
	
}"

