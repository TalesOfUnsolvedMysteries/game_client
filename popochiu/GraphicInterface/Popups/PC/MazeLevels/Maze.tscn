[sub_resource type="Shader" id=37]
code = "shader_type canvas_item;

void fragment() {
	float y = mod(TIME*0.6,1.0);
	if(UV.y + y >= 1.0) {
		y = y -1.0;
	}
	
	vec2 uv = UV + vec2(0.0, y);
	COLOR = texture(TEXTURE, uv);
}"

